{
    "manifest_version": 3,
    "name": "Leetcode Local Cloner",
    "description": "Extension to clone Leetcode questions in the local file system",
    "version": "1.0",
    "permissions": [
        "activeTab"
    ],
    "action": {
        "default_popup": "public/index.html"   
    },
    "background": {
        "service_worker": "background/cloner.js",
        "type": "module"
    }
  }